{
	"$schema": "https://schemas.gc.org/trunk/block.json",
	"apiVersion": 3,
	"name": "core/embed",
	"title": "嵌入",
	"category": "embed",
	"description": "添加可显示抖音、优酷等系统嵌入内容的区块。",
	"textdomain": "default",
	"attributes": {
		"url": {
			"type": "string",
			"__experimentalRole": "content"
		},
		"caption": {
			"type": "string",
			"source": "html",
			"selector": "figcaption",
			"__experimentalRole": "content"
		},
		"type": {
			"type": "string",
			"__experimentalRole": "content"
		},
		"providerNameSlug": {
			"type": "string",
			"__experimentalRole": "content"
		},
		"allowResponsive": {
			"type": "boolean",
			"default": true
		},
		"responsive": {
			"type": "boolean",
			"default": false,
			"__experimentalRole": "content"
		},
		"previewable": {
			"type": "boolean",
			"default": true,
			"__experimentalRole": "content"
		}
	},
	"supports": {
		"align": true,
		"spacing": {
			"margin": true
		}
	},
	"editorStyle": "gc-block-embed-editor",
	"style": "gc-block-embed"
}
