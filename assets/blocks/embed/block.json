{
	"apiVersion": 2,
	"name": "core/embed",
	"title": "嵌入",
	"category": "embed",
	"description": "添加可显示 Twitter、YouTube等站点嵌入内容的区块。",
	"textdomain": "default",
	"attributes": {
		"url": {
			"type": "string"
		},
		"caption": {
			"type": "string",
			"source": "html",
			"selector": "figcaption"
		},
		"type": {
			"type": "string"
		},
		"providerNameSlug": {
			"type": "string"
		},
		"allowResponsive": {
			"type": "boolean",
			"default": true
		},
		"responsive": {
			"type": "boolean",
			"default": false
		},
		"previewable": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"align": true
	},
	"editorStyle": "gc-block-embed-editor",
	"style": "gc-block-embed"
}
